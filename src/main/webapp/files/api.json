                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "content",
                        "in": "formData",
                        "description": "Video Description",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "404": {
                        "description": "Method Not Found"
                    },
                    "405": {
                        "description": "Method Not Allowed. Only POST is allowed"
                    },
                    "200": {
                        "description": "The POST call was successful"
                    }
                }
            }
