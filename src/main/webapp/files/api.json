        "/media/api/{id}": {
            "get": {
                "tags": [
                    "Media"
                ],
                "summary": "Returns a JSON representation of a WP Post with its associated Featured Media, if it has one",
                "operationId": "api/{id}",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Post Id",
                        "required": true,
