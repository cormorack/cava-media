        "/media/api": {
            "get": {
                "tags": [
                    "Media"
                ],
                "summary": "Returns JSON or geoJSON representations of WP_Posts",
                "operationId": "api",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "max",
                        "in": "query",
                        "description": "Max amount for paging. The default and maximum are 100.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "offset",
                        "in": "query",
                        "description": "Offset amount for paging. The default is 0.",
